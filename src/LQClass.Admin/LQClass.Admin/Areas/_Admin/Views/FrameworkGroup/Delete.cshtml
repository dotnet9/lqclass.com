@model WalkingTec.Mvvm.Mvc.Admin.ViewModels.FrameworkGroupVMs.FrameworkGroupVM
@using Microsoft.Extensions.Localization;
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
  <wt:quote>@Localizer["Sys.DeleteConfirm"]</wt:quote>
  <wt:display field="Entity.GroupCode" />
  <wt:display field="Entity.GroupName" />
  <wt:textarea field="Entity.GroupRemark" disabled="true" />
  <wt:hidden field="Entity.ID" />
  <wt:row align=" AlignEnum.Right">
    <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="@Localizer["Sys.Delete"]" />
    <wt:closebutton />
  </wt:row>
</wt:form>

