@model WalkingTec.Mvvm.Mvc.Admin.ViewModels.FrameworkRoleVMs.FrameworkRoleVM
@using Microsoft.Extensions.Localization;
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
  <wt:quote>@Localizer["Sys.DeleteConfirm"]</wt:quote>
  <wt:display field="Entity.RoleCode" />
  <wt:display field="Entity.RoleName" />
  <wt:textarea field="Entity.RoleRemark" disabled="true" />
  <wt:hidden field="Entity.ID" />
  <wt:row align=" AlignEnum.Right">
    <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除" />
    <wt:closebutton />
  </wt:row>
</wt:form>

